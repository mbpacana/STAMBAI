<AnchorPane fx:id="ap" prefHeight="700.0" prefWidth="1000.0" style="-fx-background-color: #F5F6E6;" stylesheets="@../style.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ReportsController">
    <children>
        <!-- BACKGROUND IMAGE END -->
        <!-- LEFT PANE -->
        <AnchorPane layoutY="-1.0" prefHeight="711.0" prefWidth="90.0" style="-fx-background-color: #353857;">
            <children>
                <Button fx:id="toggleButton" layoutY="245.0" mnemonicParsing="false" onAction="#switchToReports" prefHeight="91.0" prefWidth="90.0" styleClass="buttonLeft">
                    <graphic>
                        <ImageView fitHeight="65.0" fitWidth="56.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../img/reportsButton.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="reportsButton" layoutY="59.0" mnemonicParsing="false" onAction="#switchToRealtime" prefHeight="91.0" prefWidth="90.0" styleClass="buttonLeft">
                    <graphic>
                        <ImageView fitHeight="67.0" fitWidth="57.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../img/realtimeButton.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="directoryButton" layoutY="152.0" mnemonicParsing="false" onAction="#switchToDirectory" prefHeight="91.0" prefWidth="90.0" styleClass="buttonLeft">
                    <graphic>
                        <ImageView fitHeight="64.0" fitWidth="56.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../img/directoryButton.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
            </children></AnchorPane>
        <!-- HANi LOGO END -->
        <!-- TEXT AREA -->

        <TitledPane animated="false" layoutX="120.0" layoutY="35.0" prefHeight="641.0" prefWidth="545.0" text="untitled">
            <content>
                <ScrollPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                   <content>
                       <AnchorPane fx:id="apView" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                       <children>
                           <TextFlow fx:id="display" layoutX="-2.0" layoutY="-2.0" prefHeight="618.0" prefWidth="545.0" />
                       </children>
                       </AnchorPane>
                   </content>
                </ScrollPane>
            </content>
        </TitledPane>

        <Pane layoutX="685.0" layoutY="229.0" prefHeight="235.0" prefWidth="200.0">
            <children>

                <CheckBox fx:id="violationCheckBox" disable="false" layoutX="19.0" layoutY="118.0" mnemonicParsing="false" text="Violation" />
                <CheckBox fx:id="feesCheckBox" disable="false" layoutX="114.0" layoutY="118.0" mnemonicParsing="false" text="Fees" />
                <DatePicker fx:id="from" layoutX="53.0" layoutY="37.0" prefHeight="25.0" prefWidth="112.0" promptText="mm/dd/yyyy" />
                <DatePicker fx:id="to" layoutX="55.0" layoutY="73.0" prefHeight="25.0" prefWidth="112.0" promptText="mm/dd/yyyy" />
                <Button fx:id="displayReport" layoutX="14.0" layoutY="188.0" mnemonicParsing="false" text="Display Report" />
                <Button fx:id="saveFile" layoutX="13.0" layoutY="151.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="93.0" text="Save Report" />
                <Label layoutX="5.0" layoutY="6.0" text="Generate Summary Report for : " />
                <Label layoutX="9.0" layoutY="41.0" text="Start :" />
                <Label layoutX="10.0" layoutY="77.0" text="End:" />
            </children>
        </Pane>
        <!-- TEXT AREA END-->
        <!-- GENERATE REPORT AREA END-->

    </children>
</AnchorPane>
